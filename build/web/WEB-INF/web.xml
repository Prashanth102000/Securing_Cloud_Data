<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <servlet>
        <servlet-name>fileuploadd</servlet-name>
        <servlet-class>com.db.Connection.fileuploadd</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Download1</servlet-name>
        <servlet-class>com.db.Connection.Download1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>download2</servlet-name>
        <servlet-class>com.db.Connection.download2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>fileuploaddd</servlet-name>
        <servlet-class>com.db.Connection.fileuploaddd</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>fileuploadd</servlet-name>
        <url-pattern>/fileuploadd</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Download1</servlet-name>
        <url-pattern>/Download1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>download2</servlet-name>
        <url-pattern>/download2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>fileuploaddd</servlet-name>
        <url-pattern>/fileuploaddd</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
